{
  "manifest_version": 2,

  "name": "Bronto: Getting Started",
  "description": "This is a quick start guide for the Bronto Marketing Platform",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Bronto: Getting Started"
  },
  "permissions": [
    "tabs",
    "https://ajax.googleapis.com/",
    "bookmarks",
    "webNavigation"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"

}


